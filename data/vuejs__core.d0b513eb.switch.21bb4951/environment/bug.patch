+ def count_divisors(n):
+     """
+     Count the number of divisors of a positive integer n.
+    
+     Args:
+         n: A positive integer
+        
+     Returns:
+         The number of divisors of n
+     """
+     count = 0
+     i = 1
+     while i * i <= n:
+         if n % i == 0:
+             if i * i == n:
+                 count += 1
+             else:
+                 count += 2
+         i += 1
+     return count
     }
