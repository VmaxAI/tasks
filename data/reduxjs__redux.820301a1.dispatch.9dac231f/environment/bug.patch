+ def merge_intervals(intervals):
+     """
+     Merge all overlapping intervals.
+    
+     Args:
+     intervals: List[List[int]] - list of intervals where each interval is [start, end]
+    
+     Returns:
+     List[List[int]] - list of merged intervals
+    
+     Example:
+     >>> merge_intervals([[1,3],[2,6],[8,10],[15,18]])
+     [[1,6],[8,10],[15,18]]
+     """
+     if not intervals:
+         return []
+    
+     # Sort intervals by start time
+     intervals.sort(key=lambda x: x[0])
+    
+     merged = []
+     current_interval = intervals[0]
+    
+     for i in range(1, len(intervals)):
+         # Check if current interval overlaps with the next one
+         if current_interval[1] >= intervals[i][0]:
+             # Merge by extending the end to the maximum of both ends
+             current_interval[1] = max(current_interval[1], intervals[i][1])
+         else:
+             # No overlap, add current interval to result and move to next
+             merged.append(current_interval)
+             current_interval = intervals[i]
+    
+     # Don't forget to add the last interval
+     merged.append(current_interval)
+    
+     return merged
     }
