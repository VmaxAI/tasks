+def is_valid_sudoku(board):
+    """
+    Check if a 9x9 Sudoku board is valid.
+    
+    Args:
+        board: A 9x9 2D list representing the Sudoku board.
+               Empty cells are represented by 0.
+    
+    Returns:
+        True if the board is valid, False otherwise.
+    """
+    # Check rows
+    for row in board:
+        values = [val for val in row if val != 0]
+        if len(values) != len(set(values)):
+            return False
+    
+    # Check columns
+    for col in range(9):
+        values = [board[row][col] for row in range(9) if board[row][col] != 0]
+        if len(values) != len(set(values)):
+            return False
+    
+    # Check 3x3 sub-boxes
+    for box_row in range(3):
+        for box_col in range(3):
+            values = []
+            for i in range(3):
+                for j in range(3):
+                    val = board[box_row * 3 + i][box_col * 3 + j]
+                    if val != 0:
+                        values.append(val)
+            if len(values) != len(set(values)):
+                return False
+    
+    return True
