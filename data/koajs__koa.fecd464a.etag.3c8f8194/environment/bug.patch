+ def find_peak(arr):
+     """
+     Find a peak element in the array.
+     A peak element is an element that is greater than or equal to its neighbors.
+     For corner elements, we only consider one neighbor.
+    
+     Args:
+         arr: A list of integers
+        
+     Returns:
+         The index of a peak element, or -1 if the array is empty
+     """
+     if not arr:
+         return -1
+    
+     n = len(arr)
+    
+     if n == 1:
+         return 0
+    
+     # Check if first element is a peak
+     if arr[0] >= arr[1]:
+         return 0
+    
+     # Check if last element is a peak
+     if arr[n-1] >= arr[n-2]:
+         return n - 1
+    
+     # Check for peak in the middle
+     for i in range(1, n-1):
+         if arr[i] >= arr[i-1] and arr[i] >= arr[i+1]:
+             return i
+    
+     return -1
 
