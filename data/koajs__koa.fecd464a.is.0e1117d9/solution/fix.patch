+ def find_min_rotated_sorted_array(nums):
+     """
+     Find the minimum element in a rotated sorted array with possible duplicates.
+    
+     Args:
+     nums: List[int] - A rotated sorted array that may contain duplicates
+    
+     Returns:
+     int - The minimum element in the array
+    
+     Examples:
+     >>> find_min_rotated_sorted_array([3, 4, 5, 1, 2])
+     1
+     >>> find_min_rotated_sorted_array([2, 2, 2, 0, 1])
+     0
+     >>> find_min_rotated_sorted_array([1, 3, 5])
+     1
+     """
+     left = 0
+     right = len(nums) - 1
+    
+     while left < right:
+         mid = left + (right - left) // 2
+        
+         if nums[mid] > nums[right]:
+             # Minimum is in the right half
+             left = mid + 1
+         elif nums[mid] < nums[right]:
+             # Minimum is in the left half (including mid)
+             right = mid
+         else:
+             # nums[mid] == nums[right], can't determine, reduce search space
+             right -= 1
+    
+     return nums[left]
 
