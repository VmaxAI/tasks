+ def move_zeros(lst):
+     """
+     Move all zeros in the list to the end while maintaining
+     the relative order of non-zero elements.
+    
+     Args:
+         lst: A list of integers
+    
+     Returns:
+         A new list with zeros moved to the end
+    
+     Example:
+         >>> move_zeros([1, 0, 2, 0, 3])
+         [1, 2, 3, 0, 0]
+     """
+     non_zero = [x for x in lst if x != 0]
+     zero_count = len(lst) - len(non_zero)
+     return non_zero + [0] * zero_count
 
