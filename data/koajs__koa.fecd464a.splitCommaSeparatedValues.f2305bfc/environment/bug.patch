+def find_min_rotations(s: str, t: str) -> int:
+    """
+    Find the minimum number of rotations needed to transform string s into string t.
+    A rotation is moving the first character to the end.
+    
+    Args:
+        s: Source string
+        t: Target string
+    
+    Returns:
+        Minimum number of rotations needed, or -1 if impossible
+    """
+    # Check if lengths are equal
+    if len(s) != len(t):
+        return -1
+    
+    # Check if strings are empty
+    if len(s) == 0:
+        return 0
+    
+    # Check if the strings have the same characters with same frequencies
+    if sorted(s) != sorted(t):
+        return -1
+    
+    # Try each possible rotation
+    for rotations in range(len(s)):
+        # Perform rotation: move first 'rotations' characters to the end
+        rotated = s[rotations:] + s[:rotations]
+        if rotated == t:
+            return rotations
+    
+    # If no rotation works, return -1
+    return -1
