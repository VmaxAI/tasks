+def find_min_rotated(arr):
+    """
+    Find the minimum element in a rotated sorted array.
+    
+    Args:
+        arr: A rotated sorted array (originally sorted in ascending order)
+        
+    Returns:
+        The minimum element in the array
+    """
+    if not arr:
+        return None
+    
+    left, right = 0, len(arr) - 1
+    
+    # If array is not rotated or has only one element
+    if arr[left] <= arr[right]:
+        return arr[left]
+    
+    while left < right:
+        mid = (left + right) // 2
+        
+        # Check if mid+1 is the minimum
+        if mid < len(arr) - 1 and arr[mid] > arr[mid + 1]:
+            return arr[mid + 1]
+        
+        # Check if mid itself is the minimum
+        if mid > 0 and arr[mid] < arr[mid - 1]:
+            return arr[mid]
+        
+        # Decide which half to search
+        if arr[mid] > arr[right]:
+            # Minimum is in the right half
+            left = mid + 1
+        else:
+            # Minimum is in the left half
+            right = mid
+    
+    return arr[left]
