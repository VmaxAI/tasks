+ def count_vowels_consonants(s: str) -> tuple:
+     """
+     Count the number of vowels and consonants in a string.
+    
+     Args:
+         s: Input string
+        
+     Returns:
+         A tuple (vowel_count, consonant_count)
+     """
+     vowels = "aeiouAEIOU"
+     vowel_count = 0
+     consonant_count = 0
+    
+     for char in s:
+         if char.isalpha():
+             if char in vowels:
+                 vowel_count += 1
+             else:
+                 consonant_count += 1
+    
