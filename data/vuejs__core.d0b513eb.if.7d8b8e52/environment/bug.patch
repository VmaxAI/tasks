+ def count_unique_paths(m: int, n: int) -> int:
+     """
+     Count the number of unique paths in an m x n grid from the top-left 
+     corner to the bottom-right corner, where you can only move right or down.
+    
+     Args:
+         m: Number of rows in the grid
+         n: Number of columns in the grid
+    
+     Returns:
+         The number of unique paths from top-left to bottom-right
+     """
+     # Create a 2D DP table
+     dp = [[0] * n for _ in range(m)]
+    
+     # Initialize first row - only one way to reach any cell (move right)
+     for j in range(n):
+         dp[0][j] = 1
+    
+     # Initialize first column - only one way to reach any cell (move down)
+     for i in range(m):
+         dp[i][0] = 1
+    
+     # Fill the rest of the table
+     # For each cell, paths = paths from above + paths from left
+     for i in range(1, m):
+         for j in range(1, n):
+             dp[i][j] = dp[i-1][j] + dp[i][j-1]
+    
+     # Return the number of paths to bottom-right corner
+     return dp[m-1][n-1]
 
