+def group_anagrams(strs):
+    """
+    Group anagrams together from a list of strings.
+    
+    Args:
+        strs: List of strings
+        
+    Returns:
+        List of lists, where each inner list contains strings that are anagrams
+    """
+    anagram_groups = {}
+    
+    for s in strs:
+        # Sort the characters in the string to create a key
+        # Anagrams will have the same sorted characters
+        key = tuple(sorted(s))
+        
+        if key not in anagram_groups:
+            anagram_groups[key] = []
+        
+        anagram_groups[key].append(s)
+    
+    # Return all the grouped anagrams as a list of lists
+    return list(anagram_groups.values())
 
