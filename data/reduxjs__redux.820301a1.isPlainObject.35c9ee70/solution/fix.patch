+def solve(arr):
+    """
+    Find the length of the longest increasing subsequence where all elements are even.
+    
+    Args:
+        arr: List of integers
+        
+    Returns:
+        Integer representing the length of the longest increasing subsequence of even numbers
+    """
+    # Filter out even numbers
+    evens = [x for x in arr if x % 2 == 0]
+    
+    if not evens:
+        return 0
+    
+    # Find LIS of even numbers using binary search approach
+    # tails[i] represents the smallest tail of all increasing subsequences of length i+1
+    tails = []
+    
+    for num in evens:
+        # Binary search to find the position where num should be placed
+        left, right = 0, len(tails)
+        while left < right:
+            mid = (left + right) // 2
+            if tails[mid] < num:
+                left = mid + 1
+            else:
+                right = mid
+        
+        # If left == len(tails), we extend the LIS
+        if left == len(tails):
+            tails.append(num)
+        else:
+            # Replace the element at position left with num
+            tails[left] = num
+    
+    return len(tails)
