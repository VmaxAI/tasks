+ def find_min_rotated_sorted_array(nums):
+     """
+     Find the minimum element in a rotated sorted array.
+    
+     Args:
+     nums: A list of integers representing a rotated sorted array
+    
+     Returns:
+     The minimum element in the array
+     """
+     left, right = 0, len(nums) - 1
+    
+     while left < right:
+         # If the segment is already sorted, the leftmost element is minimum
+         if nums[left] < nums[right]:
+             return nums[left]
+        
+         mid = (left + right) // 2
+        
+         # If mid element is greater than rightmost, minimum is in right half
+         if nums[mid] > nums[right]:
+             left = mid + 1
+         else:
+             # Otherwise, minimum is in left half (including mid)
+             right = mid
+    
+     return nums[left]
 }
 
