+ def find_min_rotation(s: str, t: str) -> int:
+     """
+     Find the minimum number of rotations to transform string s into string t.
+     A rotation is moving the last character to the front.
+    
+     Args:
+         s: source string
+         t: target string
+    
+     Returns:
+         Minimum number of rotations needed, or -1 if impossible
+    
+     Examples:
+         >>> find_min_rotation("abc", "bca")
+         2
+         >>> find_min_rotation("abc", "abc")
+         0
+         >>> find_min_rotation("abc", "def")
+         -1
+     """
+     # Check if strings have same length
+     if len(s) != len(t):
+         return -1
+    
+     # Check if strings have same character counts
+     if sorted(s) != sorted(t):
+         return -1
+    
+     # Check if already equal
+     if s == t:
+         return 0
+    
+     # Try rotations
+     current = s
+     for rotations in range(1, len(s) + 1):
+         # Rotate: move last character to front
+         current = current[-1] + current[:-1]
+         if current == t:
+             return rotations
+    
+     return -1
 
