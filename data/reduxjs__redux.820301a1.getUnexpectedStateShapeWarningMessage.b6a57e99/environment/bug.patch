+def get_max_profit(prices):
+    """
+    Calculate the maximum profit from buying and selling stock once.
+    
+    Args:
+        prices: List of integers representing stock prices on different days
+        
+    Returns:
+        Maximum profit possible, or 0 if no profit can be made
+        
+    Example:
+        >>> get_max_profit([7, 1, 5, 3, 6, 4])
+        5
+        >>> get_max_profit([7, 6, 4, 3, 1])
+        0
+    """
+    if not prices or len(prices) < 2:
+        return 0
+    
+    min_price = prices[0]
+    max_profit = 0
+    
+    for price in prices[1:]:
+        # Calculate profit if we sell at current price
+        profit = price - min_price
+        # Update max profit
+        max_profit = max(max_profit, profit)
+        # Update minimum price seen so far
+        min_price = min(min_price, price)
+    
+    return max_profit
 
