+ def count_palindromic_substrings(s: str) -> int:
+     """
+     Count the number of palindromic substrings in a string.
+    
+     Args:
+         s: Input string
+        
+     Returns:
+         Number of palindromic substrings
+        
+     Examples:
+         >>> count_palindromic_substrings("abc")
+         3
+         >>> count_palindromic_substrings("aaa")
+         6
+     """
+     if not s:
+         return 0
+    
+     count = 0
+     n = len(s)
+    
+     def expand_around_center(left: int, right: int) -> int:
+         """Helper function to expand around center and count palindromes"""
+         palindrome_count = 0
+         while left >= 0 and right < n and s[left] == s[right]:
+             palindrome_count += 1
+             left -= 1
+             right += 1
+         return palindrome_count
+    
+     # Check all possible centers
+     for i in range(n):
+         # Odd length palindromes (single character center)
+         count += expand_around_center(i, i)
+         # Even length palindromes (two character center)
+         count += expand_around_center(i, i + 1)
+    
+     return count
 
