+ def find_min_rotated_sorted_array(nums):
+     """
+     Find the minimum element in a rotated sorted array.
+    
+     Args:
+     nums: A list of integers representing a rotated sorted array
+    
+     Returns:
+     The minimum element in the array
+    
+     Example:
+     >>> find_min_rotated_sorted_array([3, 4, 5, 1, 2])
+     1
+     >>> find_min_rotated_sorted_array([4, 5, 6, 7, 0, 1, 2])
+     0
+     >>> find_min_rotated_sorted_array([11, 13, 15, 17])
+     11
+     """
+     left, right = 0, len(nums) - 1
+    
+     while left < right:
+         # If the segment is already sorted, the leftmost element is minimum
+         if nums[left] < nums[right]:
+             return nums[left]
+        
+         mid = (left + right) // 2
+        
+         # If mid element is greater than the rightmost element,
+         # the minimum must be in the right half
+         if nums[mid] > nums[right]:
+             left = mid + 1
+         else:
+             # Otherwise, the minimum is in the left half (including mid)
+             right = mid
+    
+     return nums[left]
 
