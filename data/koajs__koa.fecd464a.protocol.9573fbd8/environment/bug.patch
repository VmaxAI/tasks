+ def convert_to_binary(s):
+     """
+     Convert a string to its binary representation.
+     Each character is converted to 8-bit binary.
+    
+     Args:
+         s: Input string
+        
+     Returns:
+         String of binary representations separated by spaces
+     """
+     if not s:
+         return ""
+    
+     binary_parts = []
+     for char in s:
+         # Get ASCII value and convert to binary (remove '0b' prefix)
+         binary = bin(ord(char))[2:]
+         # Pad to 8 bits
+         binary = binary.zfill(8)
+         binary_parts.append(binary)
+    
+     return ' '.join(binary_parts)
 
