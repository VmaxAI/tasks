+ def get_prime_factors(n):
+     """
+     Returns a list of prime factors of n in ascending order.
+    
+     Args:
+         n: A positive integer
+        
+     Returns:
+         A list of prime factors (with repetition) in ascending order
+     """
+     if n <= 1:
+         return []
+    
+     factors = []
+    
+     # Check for factor 2
+     while n % 2 == 0:
+         factors.append(2)
+         n = n // 2
+    
+     # Check for odd factors from 3 onwards
+     i = 3
+     while i * i <= n:
+         while n % i == 0:
+             factors.append(i)
+             n = n // i
+         i += 2
+    
+     # If n is still greater than 1, then it's a prime factor
+     if n > 1:
+         factors.append(n)
+    
+     return factors
 
