+ def maximum_product(nums):
+     """
+     Find the maximum product of three integers in the array.
+    
+     Args:
+     nums: List[int] - array of integers
+    
+     Returns:
+     int - maximum product of three integers
+     """
+     # Sort the array
+     nums.sort()
+    
+     # Maximum product can be either:
+     # 1. Product of three largest numbers
+     # 2. Product of two smallest (most negative) and largest number
+    
+     # Product of three largest
+     product1 = nums[-1] * nums[-2] * nums[-3]
+    
+     # Product of two smallest and largest
+     product2 = nums[0] * nums[1] * nums[-1]
+    
+     return max(product1, product2)
 
