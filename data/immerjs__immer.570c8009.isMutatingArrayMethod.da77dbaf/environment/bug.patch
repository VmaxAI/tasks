+ def count_substrings(string: str, substring: str) -> int:
+     """
+     Count the number of times substring appears in string (including overlaps).
+    
+     Args:
+         string: The string to search in
+         substring: The substring to search for
+    
+     Returns:
+         The number of occurrences of substring in string
+     """
+     if not substring or len(substring) > len(string):
+         return 0
+    
+     count = 0
