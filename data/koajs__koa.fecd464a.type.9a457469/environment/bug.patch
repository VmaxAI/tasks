+ def find_max_subarray(arr):
+     """
+     Find the maximum sum of a contiguous subarray.
+    
+     Args:
+         arr: List of integers
+    
+     Returns:
+         Maximum sum of contiguous subarray
+     """
+     if not arr:
+         return 0
+    
+     max_ending_here = arr[0]
+     max_so_far = arr[0]
+    
+     for i in range(1, len(arr)):
