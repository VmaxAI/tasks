+ def find_majority_element(nums):
+     """
+     Find the majority element in a list (element appearing more than n/2 times).
+    
+     Args:
+         nums: List of integers
+        
+     Returns:
+         The majority element if it exists, None otherwise
+     """
+     if not nums:
+         return None
+    
+     # Phase 1: Find candidate using Boyer-Moore Voting Algorithm
+     candidate = None
+     count = 0
+    
+     for num in nums:
+         if count == 0:
+             candidate = num
+             count = 1
+         elif num == candidate:
+             count += 1
+         else:
+             count -= 1
+    
+     # Phase 2: Verify the candidate
+     if candidate is not None:
+         actual_count = sum(1 for num in nums if num == candidate)
+         if actual_count > len(nums) // 2:
+             return candidate
+    
+     return None
 
