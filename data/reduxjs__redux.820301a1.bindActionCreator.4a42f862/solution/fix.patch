+def get_peak_memory(log: str) -> int:
+    """
+    Returns the peak memory usage in bytes from the given log string.
+    
+    Args:
+        log: A string containing log entries with memory information.
+             Memory entries are in the format "Memory: X MB"
+    
+    Returns:
+        The peak memory usage in bytes as an integer.
+    """
+    import re
+    
+    # Find all memory values in the log
+    # Pattern matches "Memory: X MB" where X can be an integer or decimal
+    pattern = r'Memory:\s*([\d.]+)\s*MB'
+    matches = re.findall(pattern, log)
+    
+    if not matches:
+        return 0
+    
+    # Convert MB to bytes and find the maximum
+    # 1 MB = 1024 * 1024 bytes
+    bytes_per_mb = 1024 * 1024
+    peak_bytes = max(float(mb) * bytes_per_mb for mb in matches)
+    
+    return int(peak_bytes)
 
