+def longest_common_substring(strings):
+    """
+    Find the longest substring that appears in all strings.
+    
+    Args:
+        strings: A list of strings
+        
+    Returns:
+        The longest common substring, or empty string if there is none
+    """
+    if not strings:
+        return ""
+    
+    if len(strings) == 1:
+        return strings[0]
+    
+    # Use the first (or shortest) string as reference
+    reference = min(strings, key=len)
+    
+    # Try all possible substrings starting from longest
+    for length in range(len(reference), 0, -1):
+        for start in range(len(reference) - length + 1):
+            substring = reference[start:start + length]
+            # Check if this substring appears in all strings
+            if all(substring in s for s in strings):
+                return substring
+    
+    return ""
