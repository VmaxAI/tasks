+ def get_max_profit(prices):
+     """
+     Given a list of stock prices, find the maximum profit that can be made
+     by buying and then selling the stock.
+    
+     Args:
+         prices: List of integers representing stock prices on different days
+        
+     Returns:
+         Maximum profit that can be achieved (0 if no profit possible)
+     """
+     if not prices or len(prices) < 2:
+         return 0
+    
+     min_price = prices[0]
+     max_profit = 0
+    
+     for price in prices[1:]:
