+ def find_min_rotated(arr):
+     """
+     Find the minimum element in a rotated sorted array.
+    
+     A rotated sorted array is an array that was originally sorted in ascending order,
+     but has been rotated at some pivot unknown to you beforehand.
+     (i.e., [0,1,2,4,5,6,7] might become [4,5,6,7,0,1,2]).
+    
+     Args:
+     arr: A list of integers representing a rotated sorted array
+    
+     Returns:
+     The minimum element in the array
+    
+     Example:
+     >>> find_min_rotated([3, 4, 5, 1, 2])
+     1
+     >>> find_min_rotated([4, 5, 6, 7, 0, 1, 2])
+     0
+     >>> find_min_rotated([11, 13, 15, 17])
+     11
+     """
+     if not arr:
+         return None
+    
+     left = 0
+     right = len(arr) - 1
+    
+     # If array is not rotated, first element is minimum
+     if arr[left] < arr[right]:
+         return arr[left]
+    
+     # Binary search for the minimum
+     while left < right:
+         mid = left + (right - left) // 2
+        
+         # If mid element is greater than right element,
+         # minimum must be in right half
+         if arr[mid] > arr[right]:
+             left = mid + 1
+         else:
+             # Otherwise, minimum is in left half (including mid)
+             right = mid
+    
+     return arr[left]
 
