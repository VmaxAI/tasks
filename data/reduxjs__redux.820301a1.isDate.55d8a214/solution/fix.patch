+def find_max_subarray(arr, low, high):
+    """
+    Find the maximum subarray sum using divide and conquer.
+    
+    Args:
+        arr: List of integers
+        low: Starting index
+        high: Ending index (inclusive)
+    
+    Returns:
+        Maximum subarray sum
+    """
+    # Base case: only one element
+    if low == high:
+        return arr[low]
+    
+    # Find the middle point
+    mid = (low + high) // 2
+    
+    # Recursively find maximum subarray sum in left half
+    left_sum = find_max_subarray(arr, low, mid)
