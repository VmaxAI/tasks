+    left_max = float('-inf')
+    current_sum = 0
+    for i in range(mid, low - 1, -1):
+        current_sum += arr[i]
+        left_max = max(left_max, current_sum)
+    
+    # Find max sum starting from mid+1 and going right
+    right_max = float('-inf')
+    current_sum = 0
+    for i in range(mid + 1, high + 1):
