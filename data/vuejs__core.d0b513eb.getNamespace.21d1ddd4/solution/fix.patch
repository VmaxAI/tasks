+ def swap_case(s):
+     """
+     Swap the case of all alphabetic characters in the string.
+    
+     Args:
+         s (str): The input string
+        
+     Returns:
+         str: String with cases swapped
+     """
+     result = ""
+     for char in s:
+         if char.isupper():
+             result += char.lower()
+         elif char.islower():
