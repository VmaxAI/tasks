+ def get_longest_increasing_subsequence(nums):
+     """
+     Returns the length of the longest strictly increasing subsequence.
+    
+     Args:
+         nums: A list of integers
+        
+     Returns:
+         An integer representing the length of the longest strictly increasing subsequence
+     """
+     if not nums:
+         return 0
+    
+     # tails[i] represents the smallest tail element of all increasing subsequences of length i+1
+     tails = []
+    
+     for num in nums:
+         # Binary search to find the position to insert/replace
+         left, right = 0, len(tails)
+        
+         while left < right:
+             mid = (left + right) // 2
+             if tails[mid] < num:
+                 left = mid + 1
+             else:
+                 right = mid
+        
+         # If left == len(tails), we found a new longest subsequence
+         if left == len(tails):
+             tails.append(num)
+         else:
+             # Replace to maintain smallest tail for this length
+             tails[left] = num
+    
+     return len(tails)
