+ def next_permutation(arr):
+     """
+     Rearranges the elements in arr to the next lexicographically greater permutation.
+     Returns True if such a permutation exists, False otherwise.
+     If False is returned, arr is left unchanged.
+    
+     Examples:
+     >>> arr = [1, 2, 3]
+     >>> next_permutation(arr)
+     True
+     >>> arr
+     [1, 3, 2]
+    
+     >>> arr = [3, 2, 1]
+     >>> next_permutation(arr)
+     False
+     >>> arr
+     [3, 2, 1]
+     """
+     n = len(arr)
+    
+     # Step 1: Find the largest index i such that arr[i] < arr[i+1]
+     i = n - 2
+     while i >= 0 and arr[i] >= arr[i + 1]:
+         i -= 1
+    
+     # If no such index exists, we're at the last permutation
+     if i < 0:
+         return False
+    
+     # Step 2: Find the largest index j > i such that arr[i] < arr[j]
+     j = n - 1
+     while j > i and arr[j] <= arr[i]:
+         j -= 1
+    
+     # Step 3: Swap arr[i] and arr[j]
+     arr[i], arr[j] = arr[j], arr[i]
+    
+     # Step 4: Reverse the suffix starting at arr[i+1]
+     left = i + 1
+     right = n - 1
+     while left < right:
+         arr[left], arr[right] = arr[right], arr[left]
+         left += 1
+         right -= 1
+    
+     return True
 
