+ def find_peak(arr):
+     """
+     Find a peak element in the array.
+     A peak element is an element that is greater than or equal to its neighbors.
+     For corner elements, we only consider the one neighbor.
+    
+     Args:
+         arr: List of integers
+    
+     Returns:
+         Index of a peak element, or -1 if array is empty
+     """
+     if not arr:
+         return -1
+    
+     if len(arr) == 1:
+         return 0
+    
+     # Check if first element is a peak
+     if arr[0] >= arr[1]:
+         return 0
+    
+     # Check if last element is a peak
+     if arr[-1] >= arr[-2]:
+         return len(arr) - 1
+    
+     # Binary search for a peak in the middle
+     left = 1
+     right = len(arr) - 2
+    
+     while left <= right:
+         mid = (left + right) // 2
+        
+         # Check if mid is a peak
+         if arr[mid] >= arr[mid - 1] and arr[mid] >= arr[mid + 1]:
+             return mid
+        
+         # If the element to the right is greater, move right
+         # (there must be a peak on the right side)
+         elif arr[mid] < arr[mid + 1]:
+             left = mid + 1
+        
+         # Otherwise, move left
+         # (there must be a peak on the left side)
+         else:
+             right = mid - 1
+    
+     return -1
 }
 
