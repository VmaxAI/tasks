+ def find_min_rotated_array(nums):
+     """
+     Find the minimum element in a rotated sorted array.
+    
+     Args:
+         nums: A list of integers representing a rotated sorted array
+        
+     Returns:
+         The minimum element in the array
+     """
+     if not nums:
+         return None
+    
+     left = 0
+     right = len(nums) - 1
+    
+     # If array is not rotated or has only one element
+     if nums[left] < nums[right] or len(nums) == 1:
+         return nums[left]
+    
+     while left < right:
+         mid = left + (right - left) // 2
+        
+         # If mid element is greater than the rightmost element,
+         # the minimum must be in the right half
+         if nums[mid] > nums[right]:
+             left = mid + 1
+         # If mid element is less than or equal to the rightmost element,
+         # the minimum could be at mid or in the left half
+         else:
+             right = mid
+    
+     return nums[left]
