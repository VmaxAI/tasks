+ def is_valid_parentheses(s: str) -> bool:
+     """
+     Check if the input string has valid parentheses.
+    
+     A string has valid parentheses if:
+     1. Every opening bracket has a corresponding closing bracket of the same type
+     2. Opening brackets are closed in the correct order
+     3. Every closing bracket has a corresponding opening bracket of the same type
+    
+     Args:
+         s: A string containing only '(', ')', '{', '}', '[', ']'
+    
+     Returns:
+         True if the parentheses are valid, False otherwise
+     """
+     stack = []
+     closing_to_opening = {')': '(', '}': '{', ']': '['}
+    
+     for char in s:
+         if char in '({[':
+             # Opening bracket - push to stack
+             stack.append(char)
+         elif char in ')}]':
+             # Closing bracket - check if it matches top of stack
+             if not stack or stack[-1] != closing_to_opening[char]:
+                 return False
+             stack.pop()
+    
+     # Valid if stack is empty (all brackets matched)
+     return len(stack) == 0
 
