+ def min_ascii_delete_sum(s1: str, s2: str) -> int:
+     """
+     Given two strings s1 and s2, return the lowest ASCII sum of deleted characters to make two strings equal.
+    
+     Args:
+     s1: First string
+     s2: Second string
+    
+     Returns:
+     Minimum sum of ASCII values of deleted characters
+    
+     Example:
+     >>> min_ascii_delete_sum("sea", "eat")
+     231
+     >>> min_ascii_delete_sum("delete", "leet")
+     403
+     """
+     m, n = len(s1), len(s2)
+    
+     # Create DP table
+     dp = [[0] * (n + 1) for _ in range(m + 1)]
+    
+     # Base case: if s2 is empty, delete all characters from s1
+     for i in range(1, m + 1):
+         dp[i][0] = dp[i-1][0] + ord(s1[i-1])
+    
+     # Base case: if s1 is empty, delete all characters from s2
+     for j in range(1, n + 1):
+         dp[0][j] = dp[0][j-1] + ord(s2[j-1])
+    
+     # Fill the DP table
+     for i in range(1, m + 1):
+         for j in range(1, n + 1):
+             if s1[i-1] == s2[j-1]:
+                 # Characters match, no deletion needed
+                 dp[i][j] = dp[i-1][j-1]
+             else:
+                 # Either delete from s1 or s2, choose minimum
+                 dp[i][j] = min(
+                     dp[i-1][j] + ord(s1[i-1]),  # Delete from s1
+                     dp[i][j-1] + ord(s2[j-1])   # Delete from s2
+                 )
+    
+     return dp[m][n]
 
