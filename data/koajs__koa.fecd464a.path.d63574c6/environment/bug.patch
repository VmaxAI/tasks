+ def find_min_rotated(arr):
+     """
+     Find the minimum element in a rotated sorted array.
+    
+     Args:
+         arr: A rotated sorted array (may contain duplicates)
+    
+     Returns:
+         The minimum element in the array, or None if array is empty
+     """
+     if not arr:
+         return None
+    
+     left = 0
+     right = len(arr) - 1
+    
+     while left < right:
+         mid = (left + right) // 2
+        
+         # If middle element is greater than rightmost element,
+         # minimum must be in the right half
+         if arr[mid] > arr[right]:
+             left = mid + 1
+         # If middle element is less than rightmost element,
+         # minimum could be at mid or in the left half
+         elif arr[mid] < arr[right]:
+             right = mid
+         # If they are equal, we can't determine which side to go,
+         # but we can safely exclude the rightmost element
+         else:
+             right -= 1
+    
+     return arr[left]
 
