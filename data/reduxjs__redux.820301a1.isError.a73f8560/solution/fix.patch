+def count_partitions(n, m):
+    """
+    Count the number of ways to partition a positive integer n 
+    using parts that are at most m.
+    
+    Args:
+        n: A positive integer to partition
+        m: Maximum value that can be used in the partition
+    
+    Returns:
+        The number of partitions of n using parts at most m
+    """
+    if n == 0:
+        return 1
+    if n < 0 or m == 0:
+        return 0
