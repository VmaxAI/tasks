+ def find_median(arr1, arr2):
+     """
+     Find the median of two sorted arrays.
+    
+     Args:
+     arr1: First sorted array
+     arr2: Second sorted array
+    
+     Returns:
+     The median value as a float
+     """
+     total_length = len(arr1) + len(arr2)
+    
+     # Handle empty arrays
+     if total_length == 0:
+         return 0.0
+    
+     # Determine which position(s) we need
+     is_odd = total_length % 2 == 1
+     mid_pos = total_length // 2
+    
+     # Two pointers for traversing both arrays
+     i, j = 0, 0
+     current = 0
+     prev = 0
+    
+     # Traverse until we reach the median position(s)
+     for count in range(mid_pos + 1):
+         prev = current
+        
+         # Take from arr1 if arr2 is exhausted or arr1 has smaller element
+         if j >= len(arr2) or (i < len(arr1) and arr1[i] <= arr2[j]):
+             current = arr1[i]
+             i += 1
+         # Take from arr2
+         else:
+             current = arr2[j]
+             j += 1
+    
+     # Return median based on odd/even length
+     if is_odd:
+         return float(current)
+     else:
+         return (prev + current) / 2.0
 
