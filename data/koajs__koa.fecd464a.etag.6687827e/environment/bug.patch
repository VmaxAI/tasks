+ def get_min_operations(n):
+     """
+     Returns the minimum number of operations needed to reach n from 1.
+     Operations allowed:
+     1. Add 1 to current number
+     2. Multiply current number by 2
+     """
+     if n == 1:
+         return 0
+    
+     from collections import deque
+    
+     queue = deque([(1, 0)])  # (current_number, operations_count)
+     visited = {1}
+    
+     while queue:
+         current, ops = queue.popleft()
+        
+         # Try adding 1
+         next_add = current + 1
+         if next_add == n:
+             return ops + 1
+         if next_add < n and next_add not in visited:
+             visited.add(next_add)
+             queue.append((next_add, ops + 1))
+        
+         # Try multiplying by 2
+         next_mul = current * 2
+         if next_mul == n:
+             return ops + 1
+         if next_mul < n and next_mul not in visited:
+             visited.add(next_mul)
+             queue.append((next_mul, ops + 1))
+    
+     return -1  # Should never reach here for valid input
 
