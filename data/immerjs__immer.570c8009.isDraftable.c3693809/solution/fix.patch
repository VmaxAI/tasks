+def count_matching_parentheses(s: str) -> int:
+    """
+    Count the number of correctly nested pairs of parentheses in a string.
+    
+    Args:
+        s: Input string containing parentheses and other characters
+        
+    Returns:
+        Number of correctly matched pairs of parentheses
+        
+    Example:
+        >>> count_matching_parentheses("((()))")
+        3
+        >>> count_matching_parentheses("(()")
+        1
+        >>> count_matching_parentheses("())()")
+        2
+    """
+    open_count = 0
+    matched_pairs = 0
+    
+    for char in s:
+        if char == '(':
+            open_count += 1
+        elif char == ')':
+            if open_count > 0:
+                open_count -= 1
+                matched_pairs += 1
+    
+    return matched_pairs
 
