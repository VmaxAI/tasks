+ def next_permutation(nums: list[int]) -> bool:
+     """
+     Rearranges numbers into the lexicographically next greater permutation.
+     If such arrangement is not possible, it must rearrange it as the lowest possible order (i.e., sorted in ascending order).
+     The replacement must be in place and use only constant extra memory.
+    
+     Args:
+         nums: A list of integers to be rearranged in place
+        
+     Returns:
+         True if a next permutation exists, False if the input was the last permutation
+        
+     Example:
+         >>> nums = [1, 2, 3]
+         >>> next_permutation(nums)
+         True
+         >>> nums
+         [1, 3, 2]
+         >>> nums = [3, 2, 1]
+         >>> next_permutation(nums)
+         False
+         >>> nums
+         [1, 2, 3]
+     """
+     n = len(nums)
+    
+     # Step 1: Find the largest index i such that nums[i] < nums[i + 1]
+     i = n - 2
+     while i >= 0 and nums[i] >= nums[i + 1]:
+         i -= 1
+    
+     # If no such index exists, the permutation is the last permutation
+     if i < 0:
+         nums.reverse()
+         return False
+    
+     # Step 2: Find the largest index j greater than i such that nums[i] < nums[j]
+     j = n - 1
+     while j > i and nums[j] <= nums[i]:
+         j -= 1
+    
+     # Step 3: Swap nums[i] and nums[j]
+     nums[i], nums[j] = nums[j], nums[i]
+    
+     # Step 4: Reverse the sequence from nums[i + 1] to the end
+     left = i + 1
+     right = n - 1
+     while left < right:
+         nums[left], nums[right] = nums[right], nums[left]
+         left += 1
+         right -= 1
+    
