+ def find_largest_palindrome(n):
+     """
+     Find the largest palindrome made from the product of two n-digit numbers.
+    
+     Args:
+         n: Number of digits (positive integer)
+        
+     Returns:
+         The largest palindromic number, or None if no palindrome exists
+     """
+     if n <= 0:
+         return None
+    
+     # Helper function to check if a number is a palindrome
+     def is_palindrome(num):
+         s = str(num)
+         return s == s[::-1]
+    
+     # Define the range of n-digit numbers
+     # For n=2: range is 10 to 99
+     # For n=3: range is 100 to 999
+     min_num = 10 ** (n - 1)
+     max_num = 10 ** n - 1
+    
+     largest_palindrome = None
+    
+     # Start from the largest numbers and work downward
+     for i in range(max_num, min_num - 1, -1):
+         # Optimization: if i * max_num is less than current largest, we can break
+         if largest_palindrome is not None and i * max_num < largest_palindrome:
+             break
+            
+         for j in range(max_num, min_num - 1, -1):
+             # Optimization: if we go below i, we've already checked this pair
+             if j > i:
+                 continue
+            
+             product = i * j
+            
+             # If product is less than current largest, no need to check smaller j
+             if largest_palindrome is not None and product < largest_palindrome:
+                 break
+            
+             if is_palindrome(product):
+                 if largest_palindrome is None or product > largest_palindrome:
+                     largest_palindrome = product
+    
+     return largest_palindrome
 
