+ def find_kth_smallest(arr, k):
+     """
+     Find the kth smallest element in an unsorted array.
+    
+     Args:
+         arr: List of integers
+         k: Position of the smallest element (1-indexed)
+    
+     Returns:
+         The kth smallest element
+     """
+     if not arr or k < 1 or k > len(arr):
+         return None
+    
+     def quickselect(left, right, k_index):
+         if left == right:
+             return arr[left]
+        
+         # Choose pivot (using last element for simplicity)
+         pivot_index = partition(left, right)
+        
+         # Check if pivot is at the kth position
+         if k_index == pivot_index:
+             return arr[k_index]
+         elif k_index < pivot_index:
+             return quickselect(left, pivot_index - 1, k_index)
+         else:
+             return quickselect(pivot_index + 1, right, k_index)
+    
+     def partition(left, right):
+         pivot = arr[right]
+         i = left
+        
+         for j in range(left, right):
+             if arr[j] <= pivot:
+                 arr[i], arr[j] = arr[j], arr[i]
+                 i += 1
+        
+         arr[i], arr[right] = arr[right], arr[i]
+         return i
+    
+     return quickselect(0, len(arr) - 1, k - 1)
 
